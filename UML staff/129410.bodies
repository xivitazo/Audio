!!!138370.cpp!!!	getLink(in link : int) : float

    if (link>=nOutLinks)
        return -1;

    pthread_mutex_lock(&mtx);
    float val= out_link(link);
    linkReady[link]=0;
    pthread_mutex_unlock(&mtx);
    return val;
!!!138498.cpp!!!	setLink(in link : int, in val : float) : void

