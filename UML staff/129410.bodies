!!!138626.cpp!!!	updateLink(in nlink : int, in value : float) : int

    if (nlink>=nInLinks && linkReady[nlink]==0)
        return -1;
    pthread_mutex_lock(&mtx);
    linkReady[nlink]=0;
    in_link(nlink,value);
    pthread_mutex_unlock(&mtx);
    return true;
!!!138754.cpp!!!	isLinkReady(in nlink : int) : bool

    if (nlink>=nOutLinks)
        return false;
    return linkReady[nlink];
